CREATE TABLE population (
    SERIES TEXT,
    NMFS_POPID INTEGER,
    Region TEXT NOT NULL,
    ESU TEXT,
    MAJOR_POPULATION_GROUP TEXT,
    POPULATION_NAME TEXT NOT NULL,
    COMMON_POPULATION_NAME TEXT,
    DISPLAY_ORDER TEXT,
    SPECIES TEXT,
    RUN_TIMING TEXT,
    STREAM_NAME TEXT,
    YEAR INTEGER NOT NULL,
    NUMBER_OF_SPAWNERS FLOAT,
    FRACWILD FLOAT,
    AGE_1_RETURNS FLOAT,
    AGE_2_RETURNS FLOAT,
    AGE_3_RETURNS FLOAT,
    AGE_4_RETURNS FLOAT,
    AGE_5_RETURNS FLOAT,
    AGE_6_RETURNS FLOAT,
    AGE_7_RETURNS FLOAT,
    METHOD TEXT,
    PRIMARY KEY (POPULATION_NAME, YEAR)
);
CREATE TABLE temperature (
   Region TEXT NOT NULL,
    Site_Name TEXT,
    Site_Number INTEGER NOT NULL,
    Daily_Mean_Water_Temp REAL,
    Date DATE NOT NULL,
    PRIMARY KEY (Site_Number, Date),
    FOREIGN KEY (Region) REFERENCES PopulationData(Region)
);

SELECT * FROM population;
SELECT * FROM temperature; 